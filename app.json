{
  "expo": {
    "name": "Mygluco",
    "slug": "mygluco",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/tugacriança.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/tugacriança.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tugacriança.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/tugacriança.png"
    },
    "install": {
      "exclude": [
        "./assets/tugacriança.png"
      ]
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "fa506c8a-8251-4884-91e6-26d4f3658d73"
      }
    }
  }
}
